{
  "name": "food-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build-css && node ./bin/www",
    "nodemon": "nodemon ./bin/www --exec \"npm run build-css\" -e js,hbs,scss",
    "build-css": "node-sass --include-path scss scss/main.scss public/css/main.css",
    "node": "node ./bin/www"

  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "mongoose": "*"
  },
  "devDependencies": {
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0"
  }
}
