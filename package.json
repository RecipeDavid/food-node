{
  "name": "food-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "start": "nodemon start.js",
    "watch-code": "nodemon start.js",
    "watch-css": "nodemon start.js --exec \"npm run build-css\" -e scss",
    "build-css": "node-sass --include-path public public/styles/main.scss public/styles/main.css",
    "node": "npm run build-css && node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-hbs": "*",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "mongoose": "*"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^3.1.0",
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0"
  }
}
